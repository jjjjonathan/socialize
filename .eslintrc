{
  "extends": ["airbnb-base", "next", "next/core-web-vitals", "prettier"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "react/display-name": "off",
    "jsx-a11y/alt-text": "off",
    "import/extensions": "off",
    "camelcase": "off"
  }
}
